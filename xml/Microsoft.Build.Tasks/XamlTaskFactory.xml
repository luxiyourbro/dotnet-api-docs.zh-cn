      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:Microsoft.Build.Tasks.XamlTaskFactory">
          <source>The task factory provider for XAML tasks.</source>
          <target state="translated">XAML 任务的任务工厂提供程序。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Tasks.XamlTaskFactory.#ctor">
          <source>Creates a task factory for XAML tasks.</source>
          <target state="translated">为 XAML 任务创建任务工厂。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Tasks.XamlTaskFactory.CleanupTask(Microsoft.Build.Framework.ITask)">
          <source>The task to clean up.</source>
          <target state="translated">要清理的任务。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Tasks.XamlTaskFactory.CleanupTask(Microsoft.Build.Framework.ITask)">
          <source>Cleans up any context or state that may have been built up for a given task.</source>
          <target state="translated">清理给定任务可能已积累的任何上下文或状态。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" extradata="MT" uid="M:Microsoft.Build.Tasks.XamlTaskFactory.CleanupTask(Microsoft.Build.Framework.ITask)">
          <source>Some factories may create an AppDomain as part of an individual task instance.</source>
          <target state="translated">某些工厂可以创建作为单个任务实例的一部分的 AppDomain。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" extradata="MT" uid="M:Microsoft.Build.Tasks.XamlTaskFactory.CleanupTask(Microsoft.Build.Framework.ITask)">
          <source>Such factories use this method to shutdown the AppDomain.</source>
          <target state="translated">此类工厂使用此方法关闭 AppDomain。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Tasks.XamlTaskFactory.CreateTask(Microsoft.Build.Framework.IBuildEngine)">
          <source>The task factory logging host that logs messages in the context of the task.</source>
          <target state="translated">记录任务上下文中的消息的任务工厂日志记录主机。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Tasks.XamlTaskFactory.CreateTask(Microsoft.Build.Framework.IBuildEngine)">
          <source>Creates an instance of the task to be used.</source>
          <target state="translated">创建要使用的任务的实例。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Tasks.XamlTaskFactory.CreateTask(Microsoft.Build.Framework.IBuildEngine)">
          <source>Returns the new task factory.</source>
          <target state="translated">返回新的任务工厂。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="P:Microsoft.Build.Tasks.XamlTaskFactory.FactoryName">
          <source>Gets the name of this task factory, to be used in error messages.</source>
          <target state="translated">获取要在错误消息中使用的此任务工厂的名称。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="P:Microsoft.Build.Tasks.XamlTaskFactory.FactoryName">
          <source>For example, Task "Mytask" failed to load from "FactoryName".</source>
          <target state="translated">例如，任务“Mytask”未能从“FactoryName”加载。</target>       </trans-unit>
        <trans-unit id="112" translate="yes" xml:space="preserve" extradata="MT" uid="P:Microsoft.Build.Tasks.XamlTaskFactory.FactoryName">
          <source>Returns the name of this task factory</source>
          <target state="translated">返回此任务工厂的名称</target>       </trans-unit>
        <trans-unit id="113" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Tasks.XamlTaskFactory.GetTaskParameters">
          <source>Gets the type information for the parameters of the task.</source>
          <target state="translated">获取任务参数的类型信息。</target>       </trans-unit>
        <trans-unit id="114" translate="yes" xml:space="preserve" uid="M:Microsoft.Build.Tasks.XamlTaskFactory.GetTaskParameters">
          <source>Returns the type information as an array of PropertyInfo.</source>
          <target state="translated">以 PropertyInfo 数组的形式返回类型信息。</target>       </trans-unit>
