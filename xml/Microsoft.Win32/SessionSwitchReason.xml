    <body>
      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:Microsoft.Win32.SessionSwitchReason">
          <source>Defines identifiers used to represent the type of a session switch event.</source>
          <target state="translated">定义用于表示会话切换事件类型的标识符。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:Microsoft.Win32.SessionSwitchReason">
          <source>The <ph id="ph1">&lt;xref:Microsoft.Win32.SessionSwitchEventArgs&gt;</ph> class uses the <ph id="ph2">&lt;xref:Microsoft.Win32.SessionSwitchReason&gt;</ph> class to represent the type of a session switch event.</source>
          <target state="translated"><ph id="ph1">&lt;xref:Microsoft.Win32.SessionSwitchEventArgs&gt;</ph>类使用<ph id="ph2">&lt;xref:Microsoft.Win32.SessionSwitchReason&gt;</ph>类来表示会话交换机事件的类型。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:Microsoft.Win32.SessionSwitchReason.ConsoleConnect">
          <source>A session has been connected from the console.</source>
          <target state="translated">会话已与控制台建立连接。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:Microsoft.Win32.SessionSwitchReason.ConsoleDisconnect">
          <source>A session has been disconnected from the console.</source>
          <target state="translated">会话已与控制台断开连接。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:Microsoft.Win32.SessionSwitchReason.RemoteConnect">
          <source>A session has been connected from a remote connection.</source>
          <target state="translated">会话已与远程连接建立连接。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:Microsoft.Win32.SessionSwitchReason.RemoteDisconnect">
          <source>A session has been disconnected from a remote connection.</source>
          <target state="translated">会话已与远程连接断开连接。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:Microsoft.Win32.SessionSwitchReason.SessionLock">
          <source>A session has been locked.</source>
          <target state="translated">会话已锁定。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:Microsoft.Win32.SessionSwitchReason.SessionLogoff">
          <source>A user has logged off from a session.</source>
          <target state="translated">用户已从会话注销。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:Microsoft.Win32.SessionSwitchReason.SessionLogon">
          <source>A user has logged on to a session.</source>
          <target state="translated">用户已登录到会话。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:Microsoft.Win32.SessionSwitchReason.SessionRemoteControl">
          <source>A session has changed its status to or from remote controlled mode.</source>
          <target state="translated">会话已将其状态更改为远程控制状态或从远程控制状态更改为当前的状态。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="F:Microsoft.Win32.SessionSwitchReason.SessionUnlock">
          <source>A session has been unlocked.</source>
          <target state="translated">会话已解锁。</target>       </trans-unit>
