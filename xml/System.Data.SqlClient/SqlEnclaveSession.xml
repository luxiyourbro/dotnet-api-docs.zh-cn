          <target state="translated"><ph id="ph1">&lt;paramref name="sessionKey" /&gt;</ph> 为 <ph id="ph2">&lt;see langword="null" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="M:System.Data.SqlClient.SqlEnclaveSession.#ctor(System.Byte[],System.Int64)">
