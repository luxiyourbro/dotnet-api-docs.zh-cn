        <trans-unit id="114" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.X509RecipientCertificateServiceElement.StoreLocation">
          <source>The default value is <ph id="ph1">&lt;see cref="F:System.Security.Cryptography.X509Certificates.StoreLocation.LocalMachine" /&gt;</ph>.</source>
          <target state="translated">默认值为 <ph id="ph1">&lt;see cref="F:System.Security.Cryptography.X509Certificates.StoreLocation.LocalMachine" /&gt;</ph>。</target>       </trans-unit>
        <trans-unit id="115" translate="yes" xml:space="preserve" uid="P:System.ServiceModel.Configuration.X509RecipientCertificateServiceElement.StoreName">
          <source>Gets or sets the name of the X.509 certificate store to open.</source>
          <target state="translated">获取或设置要打开的 X.509 证书存储区的名称。</target>       </trans-unit>
        <trans-unit id="116" translate="yes" xml:space="preserve" extradata="MT" uid="P:System.ServiceModel.Configuration.X509RecipientCertificateServiceElement.StoreName">
          <source>A <ph id="ph1">&lt;see cref="T:System.Security.Cryptography.X509Certificates.StoreName" /&gt;</ph> that contains the name of the X.509 certificate store to open.</source>
          <target state="translated">一个 <ph id="ph1">&lt;see cref="T:System.Security.Cryptography.X509Certificates.StoreName" /&gt;</ph>，包含要打开的 X.509 证书存储区的名称。</target>       </trans-unit>
