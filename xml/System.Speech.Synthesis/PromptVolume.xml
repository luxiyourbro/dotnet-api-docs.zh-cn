      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Speech.Synthesis.PromptVolume">
          <source>Enumerates values for volume levels (loudness) in prompts.</source>
          <target state="translated">枚举提示的卷级别的值。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Speech.Synthesis.PromptVolume">
          <source>Members of the <ph id="ph1">&lt;xref:System.Speech.Synthesis.PromptVolume&gt;</ph> enumeration are used by the <ph id="ph2">&lt;xref:System.Speech.Synthesis.PromptStyle.%23ctor%2A&gt;</ph> constructor, by the <ph id="ph3">&lt;xref:System.Speech.Synthesis.PromptBuilder.AppendText%2A&gt;</ph> method, and by the <ph id="ph4">&lt;xref:System.Speech.Synthesis.PromptBuilder.StartStyle%2A&gt;</ph> method to specify the volume level for spoken text.</source>
          <target state="translated">成员<ph id="ph1">&lt;xref:System.Speech.Synthesis.PromptVolume&gt;</ph>枚举由<ph id="ph2">&lt;xref:System.Speech.Synthesis.PromptStyle.%23ctor%2A&gt;</ph>构造函数，也可由<ph id="ph3">&lt;xref:System.Speech.Synthesis.PromptBuilder.AppendText%2A&gt;</ph>方法，并通过<ph id="ph4">&lt;xref:System.Speech.Synthesis.PromptBuilder.StartStyle%2A&gt;</ph>方法，以指定语言的文本的音量。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Speech.Synthesis.PromptVolume">
          <source>The <ph id="ph1">&lt;xref:System.Speech.Synthesis.PromptStyle.Volume%2A&gt;</ph> property gets the volume for a <ph id="ph2">&lt;xref:System.Speech.Synthesis.PromptStyle&gt;</ph> object using a <ph id="ph3">&lt;xref:System.Speech.Synthesis.PromptVolume&gt;</ph> instance.</source>
          <target state="translated"><ph id="ph1">&lt;xref:System.Speech.Synthesis.PromptStyle.Volume%2A&gt;</ph>属性获取的卷<ph id="ph2">&lt;xref:System.Speech.Synthesis.PromptStyle&gt;</ph>对象使用<ph id="ph3">&lt;xref:System.Speech.Synthesis.PromptVolume&gt;</ph>实例。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Speech.Synthesis.PromptVolume.Default">
          <source>Indicates the engine-specific default volume level.</source>
          <target state="translated">指示引擎特定默认音量。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Speech.Synthesis.PromptVolume.ExtraLoud">
          <source>Indicates an extra loud volume level.</source>
          <target state="translated">指示一个超大音量级别。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Speech.Synthesis.PromptVolume.ExtraSoft">
          <source>Indicates an extra soft volume level.</source>
          <target state="translated">指示一个柔和音量级别。</target>       </trans-unit>
        <trans-unit id="107" translate="yes" xml:space="preserve" uid="F:System.Speech.Synthesis.PromptVolume.Loud">
          <source>Indicates a loud volume level.</source>
          <target state="translated">指示一个大音量级别。</target>       </trans-unit>
        <trans-unit id="108" translate="yes" xml:space="preserve" uid="F:System.Speech.Synthesis.PromptVolume.Medium">
          <source>Indicates a medium volume level.</source>
          <target state="translated">指示一个中音量级别。</target>       </trans-unit>
        <trans-unit id="109" translate="yes" xml:space="preserve" uid="F:System.Speech.Synthesis.PromptVolume.NotSet">
          <source>Indicates that the volume level is not set.</source>
          <target state="translated">指示该音量级别不能设置。</target>       </trans-unit>
        <trans-unit id="110" translate="yes" xml:space="preserve" uid="F:System.Speech.Synthesis.PromptVolume.Silent">
          <source>Indicates a muted volume level.</source>
          <target state="translated">指示一个静音级别。</target>       </trans-unit>
        <trans-unit id="111" translate="yes" xml:space="preserve" uid="F:System.Speech.Synthesis.PromptVolume.Soft">
          <source>Indicates a soft volume level.</source>
          <target state="translated">指示一个柔和音量级别。</target>       </trans-unit>
