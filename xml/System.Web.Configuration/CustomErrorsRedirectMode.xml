      <group id="content" extype="content">
        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.Configuration.CustomErrorsRedirectMode">
          <source>Specifies values for how the URL of the original request is handled when a custom error page is displayed.</source>
          <target state="translated">指定当显示自定义错误页面时，如何处理原始请求的 URL 的值。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" extradata="MT" uid="T:System.Web.Configuration.CustomErrorsRedirectMode">
          <source>You use the <ph id="ph1">&lt;xref:System.Web.Configuration.CustomErrorsRedirectMode&gt;</ph> values when you set the <ph id="ph2">&lt;xref:System.Web.Configuration.CustomErrorsSection.RedirectMode%2A&gt;</ph> property.</source>
          <target state="translated">你使用<ph id="ph1">&lt;xref:System.Web.Configuration.CustomErrorsRedirectMode&gt;</ph>值在你设置时<ph id="ph2">&lt;xref:System.Web.Configuration.CustomErrorsSection.RedirectMode%2A&gt;</ph>属性。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Web.Configuration.CustomErrorsRedirectMode.ResponseRedirect">
          <source>Display the error page and change the URL of the original request.</source>
          <target state="translated">显示错误页面并更改原始请求的 URL。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Web.Configuration.CustomErrorsRedirectMode.ResponseRewrite">
          <source>Display the error page without changing the original URL.</source>
          <target state="translated">显示错误页面但不更改原始 URL。</target>       </trans-unit>
