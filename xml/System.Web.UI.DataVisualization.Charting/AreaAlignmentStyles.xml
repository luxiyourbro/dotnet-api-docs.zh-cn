        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Web.UI.DataVisualization.Charting.AreaAlignmentStyles">
          <source>Specifies the alignment styles of chart areas.</source>
          <target state="translated">指定图表区域的对齐样式。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.AreaAlignmentStyles.All">
          <source>Chart areas are aligned using all values.</source>
          <target state="translated">图表区域使用所有值进行对齐。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.AreaAlignmentStyles.AxesView">
          <source>Chart areas are aligned using their axis scale views.</source>
