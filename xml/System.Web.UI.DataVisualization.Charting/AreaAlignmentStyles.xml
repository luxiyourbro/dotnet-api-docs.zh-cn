        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.AreaAlignmentStyles.None">
          <source>Chart areas are not automatically aligned.</source>
          <target state="translated">图表区域不会自动对齐。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Web.UI.DataVisualization.Charting.AreaAlignmentStyles.PlotPosition">
