        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.DataVisualization.Charting.TextOrientation">
          <source>Specifies text orientation in a chart element.</source>
          <target state="translated">指定图表元素中的文本方向。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DataVisualization.Charting.TextOrientation.Auto">
          <source>Text orientation is automatically determined, based on the type of chart element in which the text appears.</source>
          <target state="translated">根据图表元素的类型自动确定在其中显示的文本的文本方向。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DataVisualization.Charting.TextOrientation.Horizontal">
          <source>Text is horizontal.</source>
          <target state="translated">文本是水平的。</target>       </trans-unit>
        <trans-unit id="104" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DataVisualization.Charting.TextOrientation.Rotated270">
          <source>Text is rotated 270 degrees and oriented from bottom to top.</source>
          <target state="translated">文本旋转 270 度，成为自下向上的方向。</target>       </trans-unit>
        <trans-unit id="105" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DataVisualization.Charting.TextOrientation.Rotated90">
          <source>Text is rotated 90 degrees and oriented from top to bottom.</source>
          <target state="translated">文本旋转 90 度，成为自上向下的方向。</target>       </trans-unit>
        <trans-unit id="106" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.DataVisualization.Charting.TextOrientation.Stacked">
          <source>Text characters are not rotated and are positioned one below the other.</source>
          <target state="translated">文本字符不旋转，只是逐个上下排列。</target>       </trans-unit>
