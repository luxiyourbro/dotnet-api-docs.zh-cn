        <trans-unit id="101" translate="yes" xml:space="preserve" uid="T:System.Windows.Forms.ImageLayout">
          <source>Specifies the position of the image on the control.</source>
          <target state="translated">指定控件上图像的位置。</target>       </trans-unit>
        <trans-unit id="102" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.ImageLayout.Center">
          <source>The image is centered within the control's client rectangle.</source>
          <target state="translated">图像在控件的矩形工作区中居中显示。</target>       </trans-unit>
        <trans-unit id="103" translate="yes" xml:space="preserve" uid="F:System.Windows.Forms.ImageLayout.None">
